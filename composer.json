{
    "name": "nevadskiy/quill-nova-field",
    "description": "A Laravel Nova WYSIWYG field based on a Quill editor.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Quill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nevadskiy\\Quill\\QuillServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
